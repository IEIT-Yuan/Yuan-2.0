name: "postprocessing"
backend: "python"
max_batch_size: 4
input [
  {
    name: "TOKENS_BATCH"
    data_type: TYPE_INT32
    dims: [ -1, -1 ]
  }
]
output [
  {
    name: "OUTPUT"
    data_type: TYPE_STRING
    dims: [ -1, -1 ]
  }
]

parameters {
  key: "tokenizer_dir"
  value: {
    string_value: "<tokenizer_path>"
  }
}

parameters {
  key: "tokenizer_type"
  value: {
    string_value: "yuan"
  }
}

instance_group [
    {
        count: 1
        kind: KIND_CPU
    }
]
